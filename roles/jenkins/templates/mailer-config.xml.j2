<?xml version='1.0' encoding='UTF-8'?>
<hudson.tasks.Mailer_-DescriptorImpl plugin="mailer@1.15">
  <defaultSuffix></defaultSuffix>
  <smtpAuthUsername>{{ jenkins_mailer_smtp_auth_username }}</smtpAuthUsername>
  <smtpAuthPassword>{{ jenkins_mailer_smtp_auth_password }}</smtpAuthPassword>
  <replyToAddress>{{ jenkins_mailer_reply_to }}</replyToAddress>
  <smtpHost>{{ jenkins_mailer_smtp_host }}</smtpHost>
  <useSsl>true</useSsl>
  <smtpPort>465</smtpPort>
  <charset>UTF-8</charset>
</hudson.tasks.Mailer_-DescriptorImpl>
